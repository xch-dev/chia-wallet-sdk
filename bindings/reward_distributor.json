        "type": "factory",
        "args": {
          "reward_distributor_type": "RewardDistributorType",
          "manager_or_collection_did_launcher_id": "Bytes32",
          "fee_payout_puzzle_hash": "Bytes32",
          "epoch_seconds": "u64",
          "max_seconds_offset": "u64",
          "payout_threshold": "u64",
          "fee_bps": "u64",
          "withdrawal_share_bps": "u64",
